<?xml version="1.0"?>
<block>
  <name>C4FM Demodulator</name>
  <key>p25_c4fm_demodulator_ff</key>
  <category>p25</category>
  <import>import p25</import>
  <make>p25.c4fm_demodulator_ff(input_rate=$input_rate, output_rate=$output_rate)</make>

  <param>
    <name>Input Rate</name>
    <key>input_rate</key>
    <value>48000</value>
    <type>int</type>
  </param>

  <param>
    <name>Output Rate</name>
    <key>output_rate</key>
    <value>4800</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
