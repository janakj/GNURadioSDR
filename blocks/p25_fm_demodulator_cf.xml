<?xml version="1.0"?>
<block>
  <name>FM Demodulator</name>
  <key>p25_fm_demodulator_cf</key>
  <category>p25</category>
  <import>import p25</import>
  <make>p25.fm_demodulator_cf(rate=$sample_rate, symbol_deviation=$symbol_deviation)</make>

  <param>
    <name>Sample Rate</name>
    <key>sample_rate</key>
    <value>48000</value>
    <type>int</type>
  </param>

  <param>
    <name>Symbol Deviation</name>
    <key>symbol_deviation</key>
    <value>600.0</value>
    <type>real</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
