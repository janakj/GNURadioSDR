<?xml version="1.0"?>
<block>
  <name>Arbitrary Resampler</name>
  <key>p25_arb_resampler_cc</key>
  <category>p25</category>
  <import>import p25</import>
  <make>p25.arb_resampler_cc($input_rate, $output_rate)</make>

  <param>
    <name>Input Rate</name>
    <key>input_rate</key>
    <type>int</type>
  </param>

  <param>
    <name>Output Rate</name>
    <key>output_rate</key>
    <type>int</type>
  </param>
  
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
